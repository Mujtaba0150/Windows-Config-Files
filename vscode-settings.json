{
    "editor.formatOnType": false,
    "editor.formatOnPaste": true,
    "editor.formatOnSave": true,
    "editor.minimap.enabled": false,
    "editor.snippetSuggestions": "top",
    "editor.linkedEditing": true,
    "editor.fontFamily": "Cascadia Code, 'Consolas', monospace",
    "editor.fontLigatures": true,
    "editor.guides.highlightActiveIndentation": "always",
    "editor.indentSize": "tabSize",
    "editor.tabSize": 4,
    "files.autoSave": "afterDelay",
    "files.trimFinalNewlines": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell 7": {
            "path": "D:\\Apps\\Powershell 7.0\\7\\pwsh.exe",
            "icon": "terminal-powershell"
        }
    },
    "terminal.integrated.defaultProfile.windows": "PowerShell 7",
    "workbench.editorAssociations": {
        "git-rebase-todo": "default"
    },
    "workbench.startupEditor": "none",
    "workbench.colorTheme": "Visual Studio Dark",
    "workbench.sideBar.location": "right",
    "workbench.activityBar.location": "top",
    "workbench.settings.applyToAllProfiles": [
        "inline-bookmarks.default.words.red"
    ],
    "C_Cpp.vcFormat": {
        "indent": {
            "caseLabels": true,
            "accessSpecifiers": false
        },
        "newLine": {
            "beforeOpenBrace": {
                "block": "sameLine",
                "function": "sameLine",
                "lambda": "sameLine",
                "namespace": "sameLine",
                "type": "sameLine"
            },
            "beforeWhileInDoWhile": true,
            "beforeElse": true
        },
        "space": {
            "aroundAssignmentOperator": "insert",
            "afterComma": true,
            "beforeBlockOpenBrace": true,
            "beforeFunctionOpenParenthesis": "remove",
            "beforeOpenBrace": false,
            "removeBeforeSemicolon": true,
            "afterKeywordsInControlFlowStatements": false,
            "betweenEmptyBraces": false,
            "betweenEmptySquareBrackets": false,
            "betweenEmptyParameterListParentheses": false,
            "withinSquareBrackets": false,
            "withinCastParentheses": false,
            "withinInitializerListBraces": false,
            "withinLambdaBrackets": false,
            "withinParameterListParentheses": false
        }
    },
    "better-comments.tags": [
        {
            "tag": "!",
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?",
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//",
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo",
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*",
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        }
    ],
    "github.copilot.editor.enableAutoCompletions": true,
    "inline-bookmarks.default.words.red": "//!",
    "diffEditor.ignoreTrimWhitespace": false,
    "cmake.pinnedCommands": [
        "workbench.action.tasks.configureTaskRunner",
        "workbench.action.tasks.runTask"
    ],
    "cmake.showOptionsMovedNotification": false,
    "cmake.showConfigureWithDebuggerNotification": false,
    "cmake.configureOnOpen": false,
    "git.autofetch": true,
    "git.openRepositoryInParentFolders": "always",
    "vscode-edge-devtools.webhintInstallNotification": true,
    "gitlens.ai.experimental.model": "openai:gpt-4o",
    "git.confirmSync": false,
    "vscode-edge-devtools.headless": true,
    "editor.wordWrap": "on",
    "gitlens.views.scm.grouped.views": {
        "commits": true,
        "branches": true,
        "remotes": true,
        "stashes": true,
        "tags": true,
        "worktrees": true,
        "contributors": true,
        "repositories": false,
        "searchAndCompare": true,
        "launchpad": false
    },
    "liveServer.settings.donotVerifyTags": true,
    "C_Cpp.default.cppStandard": "c++23",
    "python.createEnvironment.trigger": "off",
    "dart.openDevTools": "flutter",
    "C_Cpp.formatting": "vcFormat"
}